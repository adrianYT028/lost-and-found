# React App Configuration
REACT_APP_NAME=Lost & Found App
REACT_APP_VERSION=1.0.0

# Development Server
PORT=3001

# API Configuration
REACT_APP_API_URL=https://college-lost-and-found.vercel.app/api
REACT_APP_API_URL_PROD=https://college-lost-and-found.vercel.app/api

# Image Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_ANALYTICS=false    
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_AI_MATCHING=true

# Development Settings
GENERATE_SOURCEMAP=true
BROWSER=chrome




# Optional: Google Analytics (when implemented)
# REACT_APP_GA_TRACKING_ID=your_google_analytics_id

# Optional: Sentry Error Tracking (when implemented)  
# REACT_APP_SENTRY_DSN=your_sentry_dsn
